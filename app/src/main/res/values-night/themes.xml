        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FF3F51B5</item>
        <item name="colorPrimaryDark">#FF303F9F</item>
        <item name="colorAccent">#FFFF4081</item>
    </style>

    <style name="AppTheme.Dark.Dialog" parent="Theme.AppCompat.Dialog">
